{
    "version": 2,
    "functions": {
      "api/index.js": {
        "maxDuration": 30
      }
    },
    "headers": [
      {
        "source": "/api/(.*)",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "http://localhost:3000, http://localhost:3001, http://localhost:5173, http://localhost:5174, https://tabuloo-backend-p95l.vercel.app, https://www.tabuloo.com, https://tabuloo.com, https://www.govupalu.com, https://govupalu.vercel.app, https://govupalu.com"
          },
          {
            "key": "Access-Control-Allow-Methods",
            "value": "GET, POST, PUT, DELETE, OPTIONS"
          },
          {
            "key": "Access-Control-Allow-Headers",
            "value": "Content-Type, Authorization, x-razorpay-signature"
          },
          {
            "key": "Access-Control-Allow-Credentials",
            "value": "true"
          }
        ]
      }
    ]
  }